    var timeHandler = window.setInterval(function() {
        doRequest("/web/s/s07/bulletin!mainMsg?path=/web", "POST", callBack, null);
    }, 60000); // 1 min

    doRequest("/web/s/s07/bulletin!mainMsg?path=/web", "POST", callBack, null);
